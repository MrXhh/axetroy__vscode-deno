{
  "name": "vscode-deno",
  "displayName": "%deno.displayName%",
  "description": "%deno.description%",
  "version": "2.0.4",
  "publisher": "axetroy",
  "icon": "deno.png",
  "galleryBanner": {
    "color": "#3B3738",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axetroy/vscode-deno"
  },
  "bugs": {
    "url": "https://github.com/axetroy/vscode-deno/issues"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "deno"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onCommand:deno.restart_server"
  ],
  "main": "./client/out/client/src/extension.js",
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "typescript-deno-plugin",
        "enableForWorkspaceTypeScriptVersions": false
      }
    ],
    "commands": [
      {
        "command": "deno.restart_server",
        "title": "%deno.command.restart_server%",
        "category": "deno"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Deno",
      "properties": {
        "deno.enable": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%deno.config.enabled%",
          "scope": "resource",
          "examples": [
            true,
            false
          ]
        },
        "deno.dts_file": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "markdownDescription": "%deno.config.dts_file%",
          "scope": "resource",
          "examples": [
            "./deno.d.ts",
            "/absolute/path/to/deno.d.ts"
          ]
        },
        "deno.import_map": {
          "type": "string",
          "markdownDescription": "%deno.config.import_map%",
          "scope": "resource",
          "examples": [
            "./import_map.json",
            "/absolute/path/to/import_map.json"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "postinstall": "cd client && yarn && cd ../server && yarn && cd .. && npm run installvscode",
    "installvscode": "node ./node_modules/vscode/bin/install",
    "watch": "tsc -b -w",
    "compile": "tsc -b",
    "check": "prettier '**/*.md' '**/*.json' '**/*.ts' '**/*.yml' --check",
    "format": "prettier '**/*.md' '**/*.json' '**/*.ts' '**/*.yml' --write",
    "test": "jest --coverage",
    "test-coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "build": "npx vsce package -o ./vscode-deno.vsix --yarn",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check && npm run lint"
    }
  },
  "devDependencies": {
    "@types/deep-equal": "1.0.1",
    "@types/jest": "25.1.3",
    "@types/node": "12.12.28",
    "@types/which": "1.3.2",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "execa": "4.0.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.2",
    "vscode": "1.1.36",
    "which": "2.0.2"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "typescript-deno-plugin": "./typescript-deno-plugin",
    "vscode-nls-i18n": "^0.2.0"
  }
}
